=========================================================-->
        <%- include("parcials/footer") %>
            <script src="<%= otherLang %>../js/app.js"></script>
            <script src="<%= otherLang %>../js/intersectionObservers.js"></script>
            <script src="<%= otherLang %>../js/headerNavbar.js"></script>
