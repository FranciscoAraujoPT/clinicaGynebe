                            <p>
                                <%= agreements.advanceCare.text %><a href="http://advancecare.pt/" target="_blank">
                                        <%= agreements.advanceCare.link %>
                                    </a>
                            </p>
                            <p>
                                <%= agreements.advanceCare.text2 %>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="chevronDown">
                    <button><i class="fas fa-chevron-down"> </i></button>
                </div>
